    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^5.0.1",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.4.13",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-light-accordion": "^0.1.4",
    "react-loading": "^2.0.3",
    "react-redux": "^7.2.4",
